<div>
  <%@streamers.each do |s|%>
    <ul class="list-group all-streamers-list">
      <li class="list-group-item">
        <%=link_to s.name, edit_streamer_path(s) %>
        <button type="button" class="btn btn-danger streamer-delete-btn">
          <%= link_to "削除", "streamers/#{s.id}", method: :delete, data: {confirm: "削除してもよろしいですか？"} %>
        </button>
      </li>
      <li class="list-group-item"><%= s.x_url%></li>
      <li class="list-group-item"><%=link_to "動画追加", new_video_path(s) %></li>
    </ul>
  <%end%>

  <%= render partial: "shared/back_to_menu_btn" %>
</div>