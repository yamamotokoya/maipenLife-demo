<%= form_with model: @streamer, url: streamers_path, local: true do |f| %>
   <%= f.label "名前" %>
   <%= f.text_field :name %>
   <%= f.label "X(Twitter)アカウントURL"%>
   <%= f.text_field :x_url %>
   <%= f.label "動画のアップロード" %>
   <%= f.file_field :video, :accept => 'video/*' %>
   <%= f.submit "アップロード" %>
<% end %>
